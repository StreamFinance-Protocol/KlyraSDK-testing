{
  "name": "@klyra/chain",
  "version": "1.2.0",
  "main": "dist/src/index.js",
  "devDependencies": {
    "@klyra/eslint-config": "^1.2.0",
    "tsup": "^6.0.1",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@cosmjs/amino": "^0.32.4",
    "@cosmjs/encoding": "^0.32.4",
    "@cosmjs/math": "^0.32.4",
    "@cosmjs/proto-signing": "^0.32.4",
    "@cosmjs/stargate": "^0.32.4",
    "@cosmjs/tendermint-rpc": "^0.32.4",
    "@cosmjs/utils": "^0.32.4",
    "@klyra/proto": "^0.0.8",
    "@klyra/shared": "^1.2.0",
    "@klyra/ws": "^1.2.0",
    "@osmonauts/lcd": "^0.6.0",
    "@osmonauts/telescope": "0.80.0",
    "@scure/bip32": "^1.5.0",
    "@scure/bip39": "^1.4.0",
    "axios": "^1.7.7",
    "bech32": "^2.0.0",
    "cosmjs-types": "^0.9.0",
    "ethereum-cryptography": "^3.0.0",
    "ethers": "^6.13.4",
    "long": "^5.2.3",
    "protobufjs": ">=6.11.4"
  },
  "scripts": {
    "build": "rm -rf dist/ && npm run compile",
    "compile": "tsc",
    "dev": "tsup index.ts --format cjs,esm --watch --dts",
    "transpile": "rm -rf src/codegen && telescope transpile --protoDirs ./proto --outPath ./src/codegen --no-includeAminos --includeLCDClients --includeRPCClients",
    "lint": "eslint ."
  }
}