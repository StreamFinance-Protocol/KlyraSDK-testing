{
  "name": "@klyra/core",
  "version": "1.3.0",
  "main": "./dist/index.js",
  "devDependencies": {
    "tsup": "^6.0.1",
    "typescript": "^5.4.5",
    "@klyra/eslint-config": "1.2.0"
  },
  "dependencies": {
    "@klyra/chain": "^1.2.0",
    "@klyra/cortex": "^1.2.0",
    "@klyra/proto": "^0.0.8",
    "@klyra/shared": "^1.2.0",
    "@klyra/ws": "^1.2.0",
    "protobufjs": ">=6.11.4",
    "@osmonauts/lcd": "^0.6.0",
    "@cosmjs/stargate": "^0.32.1",
    "@cosmjs/tendermint-rpc": "^0.32.1",
    "long": "^4.0.0",
    "@osmonauts/telescope": "0.80.0",
    "@cosmjs/amino": "^0.32.1",
    "@cosmjs/encoding": "^0.32.1",
    "@cosmjs/math": "^0.32.1",
    "@cosmjs/proto-signing": "^0.32.1",
    "cosmjs-types": "^0.9.0",
    "@cosmjs/crypto": "^0.32.1",
    "xstream": "^11.14.0",
    "readonly-date": "^1.0.0",
    "@cosmjs/json-rpc": "^0.32.1"
  },
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "build": "rm -rf dist/ && npm run compile",
    "compile": "tsc",
    "transpile": "rm -rf src/codegen && telescope transpile --protoDirs ./proto --outPath ./src/codegen --no-includeAminos --includeLCDClients --includeRPCClients",
    "dev": "tsup index.ts --format cjs,esm --watch --dts",
    "lint": "eslint ."
  }
}